@{
    ViewBag.Title = "ResultsView";
}

@using cosmosui.Models
@model IEnumerable<FieldMasterInfo>

@Html.ActionLink("<<Back to Search", "Index", "Bank")
<br />
<br />

@(Html.Kendo().Grid(Model)
    .Name("grid")
    .Columns(columns =>
    {
        columns.Bound(p => p.FieldId);
        columns.Bound(p => p.HelpTextKey);
        columns.Bound(p => p.TenantId);
        columns.Bound(p => p.BeneficiaryAddressCountryCode);
    })
    .Pageable()
)